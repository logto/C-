#include <iostream>
using namespace std;

class A{
public:
    typedef unsigned int UINT;
    UINT valueA;
    A() : valueA(0){

    }
};
void func3(){
    A::UINT i = 1;
    cout << i << endl;
}

int main(){
	func3();
    return 0;
}





