#include <iostream>
using namespace std;
int main()
{
	enum num {one,two,three,four} mNum;
	mNum = three;
	switch (mNum) {
		case one:
			cout <<"mNum = one"<<endl;
			break;
		case two:
			cout <<"mNum = two"<<endl;
			break;
		case three:
			cout <<"mNum = three"<<endl;
			break;
		default:
			break;
	}
	return 0;
}


