#include <iostream>
using namespace std;

void func1()
{
    typedef unsigned int UINT;
}

void func2()
{
    UINT uValue = 5;//error C2065: 'UINT' : undeclared identifier
}

int main(){
	func1();
	func2();

    return 0;
}





